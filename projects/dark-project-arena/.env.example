# ===== TELEGRAM MTPROTO =====
# Получить на https://my.telegram.org
TELEGRAM_API_ID=
TELEGRAM_API_HASH=
TELEGRAM_PHONE=
# URL и токен MTProto сервера
MTPROTO_API_URL=http://localhost:5000
MTPROTO_API_TOKEN=

# ===== REDIS =====
# Для Docker: redis, для локальной установки: localhost
REDIS_HOST=redis
REDIS_PORT=6379
# Время жизни кеша в секундах (24 часа)
REDIS_CACHE_TTL=86400

# ===== QDRANT =====
# Для Docker: qdrant, для локальной установки: localhost
QDRANT_HOST=qdrant
QDRANT_PORT=6333
# API ключ (оставьте пустым, если не настроена аутентификация)
QDRANT_API_KEY=

# ===== BUSINESS HOURS =====
# Рабочие часы для автоматических обзвонов (МСК)
WORK_HOURS_START=9
WORK_HOURS_END=18
# 1 = понедельник, 5 = пятница
WORK_DAYS_START=1
WORK_DAYS_END=5

# ===== OTHER =====
# URL webhook'а n8n для обратных вызовов
N8N_WEBHOOK_URL=