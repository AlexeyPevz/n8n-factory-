# ===== Dark Project Arena - Cloud Environment Variables =====
# Скопируйте этот файл в .env и заполните своими значениями

# ===== N8N WORKFLOW IDs =====
# После импорта workflow в n8n, скопируйте ID из адресной строки
WF_00_WORKFLOW_ID=
WF_01_WORKFLOW_ID=
WF_02_WORKFLOW_ID=
WF_03_WORKFLOW_ID=
WF_04_WORKFLOW_ID=
WF_05_WORKFLOW_ID=
WF_06_WORKFLOW_ID=

# ===== UPSTASH REDIS (Облачный Redis) =====
# Получите на https://console.upstash.com после создания базы данных
REDIS_HOST=eu1-example-12345.upstash.io
REDIS_PORT=12345
REDIS_PASSWORD=
REDIS_TLS=true
REDIS_CACHE_TTL=86400

# ===== QDRANT CLOUD =====
# Получите на https://cloud.qdrant.io после создания кластера
QDRANT_URL=https://your-cluster-12345.eu-central.aws.cloud.qdrant.io
QDRANT_API_KEY=pk-
QDRANT_COLLECTION=dark_project_kb

# ===== GOOGLE SHEETS =====
# ID таблицы из URL: https://docs.google.com/spreadsheets/d/{ID}/edit
GOOGLE_SHEET_ID=
GOOGLE_SERVICE_ACCOUNT_EMAIL=

# ===== TELEGRAM =====
# ID групп получите через @RawDataBot
TELEGRAM_ADMIN_CHAT_ID=-100
TELEGRAM_SALES_CHAT_ID=-100
TELEGRAM_SUPPLIERS_CHAT_ID=-100

# ===== BITRIX24 =====
# Webhook URL должен заканчиваться на /
BITRIX24_WEBHOOK_URL=https://your-domain.bitrix24.ru/rest/1/your-webhook-key/

# ===== EXTERNAL APIs =====
# Yandex Maps API
YANDEX_MAPS_API_KEY=

# 2GIS API
TWOGIS_API_KEY=

# OpenRouter (основной AI провайдер)
OPENROUTER_API_KEY=sk-or-v1-

# OpenAI (fallback)
OPENAI_API_KEY=sk-proj-

# ===== EMAIL (SMTP) =====
EMAIL_PROVIDER_SMTP=smtp.gmail.com
EMAIL_PROVIDER_PORT=587
EMAIL_PROVIDER_USER=your-email@gmail.com
EMAIL_PROVIDER_PASSWORD=

# ===== MTPROTO (Telegram Userbot) =====
# URL вашего MTProto сервера
MTPROTO_API_URL=https://your-vps.com:8080
MTPROTO_API_KEY=
MTPROTO_SESSION_STRING=

# ===== WHATSAPP BUSINESS =====
WHATSAPP_BUSINESS_TOKEN=
WHATSAPP_BUSINESS_PHONE_ID=
WHATSAPP_BUSINESS_ACCOUNT_ID=

# ===== РАБОЧЕЕ ВРЕМЯ =====
WORK_HOURS_START=9
WORK_HOURS_END=18
WORK_DAYS_START=1
WORK_DAYS_END=5
TIMEZONE=Europe/Moscow

# ===== ЛИМИТЫ И НАСТРОЙКИ =====
MAX_LEADS_PER_CITY=50
MAX_MESSAGES_PER_DAY=100
RATE_LIMIT_DELAY_SECONDS=2
ERROR_NOTIFICATION_ENABLED=true

# ===== N8N CLOUD (если используете) =====
N8N_WEBHOOK_BASE_URL=https://your-instance.app.n8n.cloud/webhook

# ===== NGROK (для локальной разработки) =====
# NGROK_WEBHOOK_URL=https://12345.ngrok.io/webhook