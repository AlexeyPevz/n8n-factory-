      "id": "get-wave-config-001",
      "name": "Get Wave Config",
      "type": "n8n-nodes-base.redis",
      "typeVersion": 1,
      "position": [660, 450],
      "parameters": {
        "operation": "get",
        "key": "config:current_wave",
        "propertyName": "waveConfig"
