              "output": 1,
              "outputKey": "price",
              "expression": "={{ $json.callback_query.data === 'request_price' }}"
            },
            {
              "output": 2,
              "outputKey": "notInterested",
              "expression": "={{ $json.callback_query.data === 'not_interested' }}"
            }
          ]
        },
        "fallbackOutput": 3
